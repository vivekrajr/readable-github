{
  "manifest_version": 2,

  "name": "Beautify Github",
  "description": "This extension makes Github more readable!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["github.css"]
    }
  ]
}